<div class="panel panel-default" style="width: 500px;margin: 0 auto">
  <div class="panel-heading"><h4>Detalhe do Setor</h4></div>
  <div class="panel-body">  
   <p>
     <strong>Descrição:</strong>
  <%= @setor.descr %>
   </p>
   <% if notice %>
     <div class="alert alert-info"><p id="notice"><%= notice %></p></div> 
   <%end%>
   
  </div>
  
</div>
<ul class="pager">
   <li><%= link_to 'Editar', edit_setor_path(@setor) %></li>
   <li><%= link_to 'Voltar', setors_path %></li>
</ul>
